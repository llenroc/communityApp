<script>
    $(document).ready(function(){
        $(".center-cropped").on('mouseenter', function () {
            $(this).animate({  width: "+=5", height: "+=5" })
        });
        $(".center-cropped").on('mouseleave', function () {
            $(this).animate({  width: "-=5", height: "-=5" })
