    <script type="text/javascript">
        jQuery(function($) {
            $("#btn_CreateCommunity").click(function () {

                var communityName = $("#createCommunityName").val();
                var communityPassword = $("#communityPassword").val();
                var communityRequiredPassword = $("#communityRequiredPassword").val();
                //alert(communityName + " " + communityPassword + " " + communityRequiredPassword)
                $.ajax({
                    url: '@Url.Action("CreateCommunity", "Home")',
                    type: 'POST',
                    data: { "communityName": communityName, "communityPassword": communityPassword, "communityRequiredPassword": communityRequiredPassword },
                    dataType: 'json',

                    success: function (response) {
                        if (response.status == "Success") {
                            window.location.reload();
                        }
                        else {
                            $("#AddCommunityError").html(response.status);
                            $("#AddCommunityError").css("visibility", "visible");
                            $("#AddCommunityError").css("opacity", "1");
                            $("#AddCommunityError").fadeTo(7000, 0, function () {
                                $("#AddCommunityError").css("visibility", "hidden");
                            });
                       
                        }

                    


                    },
                    error: function (error) {
                    }
                });
            });
      
        function qtyMsg(alertMsg, p, defaultVal) {
            p.find(".alert-msg").remove();
            p.append('<span class="alert-msg">' + alertMsg + '</span>');
            if (typeof defaultVal != "undefined") {
                p.find("input").val(defaultVal);
            }
            var qtimer = setInterval(function (e) {
                if (qtimer != null) {
                    clearTimeout(qtimer);
                    qtimer = null;
                }
                p.find(".alert-msg").remove();
            }, 4000);
        }

        $('body').on("click", ".quantity-control .btn-plus", function (e) {
            var p = $(this).parent(),
                tInput = p.find("input"),
                tValue = +tInput.val(),
                maxAllowed = +tInput.attr("data-max"),
                alertMsg = tInput.attr("data-maxalert");
            tInput.val(tValue + 1);
            var emergencyContactRange = tValue+1;
      
            $.ajax({
                url: '@Url.Action("UpdateEmergencyContactRange", "SystemAdminHome")',
                type: 'POST',
                data: { "EmergencyContactRange": emergencyContactRange },
                dataType: 'json',

                success: function (response) {
                    if (response.Success == "Success") {
                        
                        //alertMsg = "update Successfully";
                        //qtyMsg(alertMsg, p);
                    }

                    else {
                        alertMsg = "did not update Successfully";
                        qtyMsg(alertMsg, p);
                    }
                        
                       
                },
                error: function (error) {
                }
            });
          
            //if (tValue < maxAllowed) {
            //    tInput.val(tValue + 1);
            //} else if (!p.find(".alert-msg").length) {
            //    qtyMsg(alertMsg, p);
            //}
        });

        $('body').on("click", ".quantity-control .btn-minus", function (e) {
            var p = $(this).parent(),
                tInput = p.find("input"),
                tValue = +tInput.val(),
                minAllowed = +tInput.attr("data-min"),
                alertMsg = tInput.attr("data-minalert");
            if (tValue > 0)
            {
                tInput.val(tValue - 1);
            }
            
            if (tValue > 0) {
               
                var emergencyContactRange = tValue - 1;
                
                $.ajax({
                    url: '@Url.Action("UpdateEmergencyContactRange", "SystemAdminHome")',
                    type: 'POST',
                    data: { "EmergencyContactRange": emergencyContactRange },
                    dataType: 'json',

                    success: function (response) {
                        if (response.Success == "Success") {
                            //alertMsg = "update Successfully";
                            //qtyMsg(alertMsg, p);
                        }

                        else {
                            alertMsg = "did not update Successfully";
                            qtyMsg(alertMsg, p);
                        }
                       

                    },
                    error: function (error) {
                    }
                });
            } else if (!p.find(".alert-msg").length) {
                qtyMsg(alertMsg, p);
            }
        });

        $(".quantity-control input").keyup(function () {
            var $this = $(this),
                p = $this.parent(),
                val = $this.val(),
                minAllowed = +$this.attr("data-min"),
                maxAllowed = +$this.attr("data-max"),
                msgMax = $this.attr("data-maxalert"),
                msgMin = $this.attr("data-minalert"),
                msginvalid = $this.attr("data-invalid");
            if ($.isNumeric(val)) {
                if (val > maxAllowed) {
                    qtyMsg(msgMax, p, maxAllowed);
                } else if (val < minAllowed) {
                    qtyMsg(msgMin, p, minAllowed);
                }
            } else {

                $this.val(minAllowed);
                qtyMsg(msginvalid, p);
            }
        });
        });
    </script>
